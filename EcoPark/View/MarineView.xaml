<Window x:Class="EcoPark.View.MarineView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EcoPark.View"
        mc:Ignorable="d"
        Title="MarineView" Height="450" Width="450">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Height="32" Margin="32,24,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="138" FontWeight="Bold"><Run Language="sv-se" Text="General "/><Run Text="Marine Data"/></TextBlock>
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="54,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="71"><Run Language="sv-se" Text="Is a predator"/></TextBlock>
        <RadioButton x:Name="radioBtnYes" Content="Yes" HorizontalAlignment="Left" Height="23" Margin="130,56,0,0" VerticalAlignment="Top" Width="40"/>
        <RadioButton Content="No" HorizontalAlignment="Left" Height="23" Margin="175,56,0,0" VerticalAlignment="Top" Width="39"/>
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="54,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="71"><Run Language="sv-se" Text="Living deph"/></TextBlock>
        <TextBox x:Name="txtLivingDeph" HorizontalAlignment="Left" Height="19" Margin="130,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="48"/>
        <TextBlock HorizontalAlignment="Left" Height="14" Margin="183,93,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="13"><Run Language="sv-se" Text="m"/></TextBlock>
        <TextBlock HorizontalAlignment="Left" Height="23" Margin="54,126,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="36"><Run Language="sv-se" Text="Color"/></TextBlock>
        <TextBox x:Name="txtColor" HorizontalAlignment="Left" Height="18" Margin="90,126,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="88"/>

    </Grid>
</Window>
